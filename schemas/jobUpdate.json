{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "properties": {
        "title": {
            "type": "string",
            "default": "",
            "title": "The title Schema",
            "examples": [
                "Engineer"
            ]
        },
        "salary": {
            "type": "number",
            "default": 0.0,
            "title": "The salary Schema",
            "examples": [
                100000.00
            ],
            "minimum": 0
        },
        "equity": {
            "type": "number",
            "default": 0.0,
            "title": "The equity Schema",
            "examples": [
                0.1
            ],
            "maximum": 1.0
        }
    },
    "examples": [{
        "title": "Engineer",
        "salary": 100000.00,
        "equity": 0.1
    }],
    "additionalProperties": false
}